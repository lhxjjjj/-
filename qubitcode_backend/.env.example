# 数据库配置
DATABASE_URL=mysql+pymysql://root:password@localhost/qubitcode

# Flask配置
FLASK_APP=app.py
FLASK_ENV=development
SECRET_KEY=your-secret-key-here

# API配置
API_VERSION=v1
API_PREFIX=/api

# 跨域配置
CORS_ORIGINS=*

# 日志配置
LOG_LEVEL=INFO
LOG_FILE=logs/app.log

# 聊天配置
MAX_MESSAGE_LENGTH=10000
MAX_SESSIONS_PER_IP=10